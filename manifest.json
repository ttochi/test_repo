{
  "manifest_version": 2,
  "name": "Tab Manager",
  "description": "Managing Chrome Tab",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "icons": {
    "128": "images/icon.jpg"
  },

  "browser_action": {
    "default_icon": "images/icon.jpg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}